{
    "manifest_version": 3,
    "name": "Darwinbox Logout Alert",
    "version": "1.0",
    "description": "Show alert on Darwinbox logout",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://impelsys.darwinbox.in/*"],
        "js": ["content.js"]
      }
    ]
}
  
  
  